package test;

import java.Numbers;
import static org.junit.Assert.assertEquals; 
import org.junit.Test;

public class ConversionTest{

	private Numbers num;
	public ConversionTest() {
		num = new Numbers();
	}
	
	@Test
	public void arabicToRomanOne(){
		assertEquals("I", "1", num.conversionA2R("1"));
	}
}
